  <security-constraint>
    <web-resource-collection>
      <web-resource-name>All resources</web-resource-name>
      <description>Protects all resources</description>
      <url-pattern>/NeverGonnaBeCall/*</url-pattern>
    </web-resource-collection>
  </security-constraint>
  <login-config>
